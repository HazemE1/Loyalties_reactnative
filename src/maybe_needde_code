await this.state.verificationID.confirm(c)
    .then((a) => {
        setErrMsg("");
        const isNew = a._tokenResponse.isNewUse
        if (isNew)
            registerUser()
        else
            signInUser()
        console.log(firebase.auth().currentUser);
    })
    .catch((e) => {
        console.log(e);
        setErrMsg("Angivet kod stämmer inte!")
        return
    })